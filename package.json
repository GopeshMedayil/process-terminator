    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "electron-packager": "^15.4.0",
    "pid-from-port": "^1.1.3",
    "wait-on": "^6.0.1",
    "electron": "^18.0.3"
